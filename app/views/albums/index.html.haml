- content_for(:heading) do
  %h1 Listing albums

%table
  %tr
    %th Title
    %th Artist
    %th Label
    %th Rdio url
    %th Spotify url
    %th
    %th
    %th
  - @albums.each do |album|
    %tr
      %td= album.title
      %td= album.artist_id
      %td= album.label_id
      %td= album.rdio_url
      %td= album.spotify_url
      %td= link_to 'Show', album
      %td= link_to 'Edit', edit_album_path(album)
      %td= link_to 'Destroy', album, confirm: 'Are you sure?', method: :delete
%br/
= link_to 'New Album', new_album_path

= javascript_tag backbone_router("albums", @albums)
