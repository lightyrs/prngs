<h1>New artist</h1>

<form id="new-artist" name="artist">
  <div class="field">
    <label for="name"> name:</label>
    <input type="text" name="name" id="name" value=<%= name %> >
  </div>

  <div class="field">
    <label for="sound"> sound:</label>
    <input type="text" name="sound" id="sound" value=<%= sound %> >
  </div>

  <div class="field">
    <label for="rdio_url"> rdio_url:</label>
    <input type="text" name="rdio_url" id="rdio_url" value=<%= rdio_url %> >
  </div>

  <div class="field">
    <label for="spotify_url"> spotify_url:</label>
    <input type="text" name="spotify_url" id="spotify_url" value=<%= spotify_url %> >
  </div>

  <div class="actions">
    <input type="submit" value="Create Artist" />
  </div>

</form>

