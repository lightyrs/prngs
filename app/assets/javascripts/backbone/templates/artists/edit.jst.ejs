<h1>Edit artist</h1>

<form id="edit-artist" name="artist">
  <div class="field">
    <label for="name"> name:</label>
    <input type="text" name="name" id="name" value=<%= name %> >
  </div>

  <div class="field">
    <label for="echonest_id"> echonest_id:</label>
    <input type="text" name="echonest_id" id="echonest_id" value=<%= echonest_id %> >
  </div>

  <div class="field">
    <label for="images"> images:</label>
    <input type="text" name="images" id="images" value=<%= images %> >
  </div>

  <div class="field">
    <label for="biographies"> biographies:</label>
    <input type="text" name="biographies" id="biographies" value=<%= biographies %> >
  </div>

  <div class="field">
    <label for="popularity"> popularity:</label>
    <input type="text" name="popularity" id="popularity" value=<%= popularity %> >
  </div>

  <div class="field">
    <label for="familiarity"> familiarity:</label>
    <input type="text" name="familiarity" id="familiarity" value=<%= familiarity %> >
  </div>

  <div class="actions">
    <input type="submit" value="Update Artist" />
  </div>

</form>

