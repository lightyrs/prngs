<h1>New label</h1>

<form id="new-label" name="label">
  <div class="field">
    <label for="name"> name:</label>
    <input type="text" name="name" id="name" value=<%= name %> >
  </div>

  <div class="field">
    <label for="rdio_url"> rdio_url:</label>
    <input type="text" name="rdio_url" id="rdio_url" value=<%= rdio_url %> >
  </div>

  <div class="actions">
    <input type="submit" value="Create Label" />
  </div>

</form>

