<h1>Edit mention</h1>

<form id="edit-mention" name="mention">
  <div class="field">
    <label for="track_id"> track_id:</label>
    <input type="text" name="track_id" id="track_id" value=<%= track_id %> >
  </div>

  <div class="field">
    <label for="album_id"> album_id:</label>
    <input type="text" name="album_id" id="album_id" value=<%= album_id %> >
  </div>

  <div class="field">
    <label for="source_id"> source_id:</label>
    <input type="text" name="source_id" id="source_id" value=<%= source_id %> >
  </div>

  <div class="field">
    <label for="author_id"> author_id:</label>
    <input type="text" name="author_id" id="author_id" value=<%= author_id %> >
  </div>

  <div class="field">
    <label for="text"> text:</label>
    <input type="text" name="text" id="text" value=<%= text %> >
  </div>

  <div class="field">
    <label for="url"> url:</label>
    <input type="text" name="url" id="url" value=<%= url %> >
  </div>

  <div class="field">
    <label for="rating"> rating:</label>
    <input type="text" name="rating" id="rating" value=<%= rating %> >
  </div>

  <div class="field">
    <label for="sentiment"> sentiment:</label>
    <input type="text" name="sentiment" id="sentiment" value=<%= sentiment %> >
  </div>

  <div class="field">
    <label for="date"> date:</label>
    <input type="text" name="date" id="date" value=<%= date %> >
  </div>

  <div class="actions">
    <input type="submit" value="Update Mention" />
  </div>

</form>

<a href="#/index">Back</a>