<h1>New source</h1>

<form id="new-source" name="source">
  <div class="field">
    <label for="name"> name:</label>
    <input type="text" name="name" id="name" value=<%= name %> >
  </div>

  <div class="field">
    <label for="url"> url:</label>
    <input type="text" name="url" id="url" value=<%= url %> >
  </div>

  <div class="field">
    <label for="kind"> kind:</label>
    <input type="text" name="kind" id="kind" value=<%= kind %> >
  </div>

  <div class="field">
    <label for="feeds"> feeds:</label>
    <input type="text" name="feeds" id="feeds" value=<%= feeds %> >
  </div>

  <div class="field">
    <label for="popularity"> popularity:</label>
    <input type="text" name="popularity" id="popularity" value=<%= popularity %> >
  </div>

  <div class="actions">
    <input type="submit" value="Create Source" />
  </div>

</form>

<a href="#/index">Back</a>